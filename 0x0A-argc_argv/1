#include "main.h"
#include <stdio.h>

/**
 * main -it prints the name of the program
 * @argc: is the number of arguments passed to the function
 * @argv: the array of pointers to the arguments
 *
 * Return: Always 0 (Success)
 */
int main(int argc, char **argv)
{
	printf("%s\n", *argv);

	return 0;
}


